<?xml version="1.0" encoding="utf-8"?>
<mx:Canvas xmlns:mx="http://www.adobe.com/2006/mxml" width="100%" height="100%" xmlns:control="CONTROL.*" creationPolicy="all">
	<mx:Script>
		<![CDATA[
			import Data.Globales;
			
			public function muestraPdf(consulta:String):void{
				trace(consulta);
				//ocultaComp();	
				iFrame.source = consulta; 
				iFrame.visible = true;
			}
			
			public function ocultarPdf():void{	
				iFrame.source = ""; 
				iFrame.visible = false;
			}
		]]>
	</mx:Script>
	<control:IFrameB id="iFrame" x="0" y="10" width="100%" height="100%"/>
</mx:Canvas>